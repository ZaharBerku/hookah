{
  "kind": "collectionType",
  "collectionName": "colors",
  "info": {
    "singularName": "color",
    "pluralName": "colors",
    "displayName": "color",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "pluginOptions": {},
      "type": "string",
      "required": true
    },
    "imageColor": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ],
      "pluginOptions": {}
    },
    "color": {
      "pluginOptions": {},
      "type": "customField",
      "regex": "^#([A-Fa-f0-9]{6}|[A-Fa-f0-9]{3})$",
      "customField": "plugin::color-picker.color"
    },
    "slug": {
      "pluginOptions": {},
      "type": "uid",
      "targetField": "name"
    },
    "flask": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::flask.flask",
      "inversedBy": "colors"
    }
  }
}
