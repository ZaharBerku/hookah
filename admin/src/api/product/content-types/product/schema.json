{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "name": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "required": true,
      "unique": false
    },
    "category": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "enumeration",
      "enum": [
        "coal",
        "hookah",
        "accessories",
        "tobacco",
        "liquid",
        "vape"
      ],
      "required": true,
      "default": "tobacco"
    },
    "likes": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "biginteger",
      "required": true,
      "min": "0"
    },
    "price": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "biginteger",
      "required": true,
      "min": "0",
      "default": "0"
    },
    "discount": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "biginteger",
      "max": "100",
      "min": "0",
      "required": false,
      "default": "0"
    },
    "description": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "text",
      "required": false
    },
    "previewImage": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "gallery": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "available": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "boolean",
      "default": true,
      "required": true
    },
    "numberOf": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "biginteger",
      "min": "0",
      "required": true,
      "default": "0"
    },
    "producer": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "enumeration",
      "enum": [
        "twoXtwo"
      ],
      "default": "twoXtwo",
      "required": true
    },
    "country": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "customField",
      "required": false,
      "customField": "plugin::country-select.country"
    },
    "test": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "dynamiczone",
      "components": [
        "hookah.hookah",
        "tobacco.tobacco"
      ]
    }
  }
}
